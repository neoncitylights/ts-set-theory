{
	"name": "@neoncitylights/set-theory",
	"version": "1.3.0",
	"description": "TS library for set theory mathematics",
	"author": {
		"name": "neoncitylights",
		"url": "https://github.com/neoncitylights"
	},
	"keywords": [
		"sets",
		"set-theory",
		"discrete-mathematics"
	],
	"license": "MIT",
	"bugs": "https://github.com/neoncitylights/ts-set-theory/issues",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/neoncitylights/ts-set-theory.git"
	},
	"funding": {
		"type": "individual",
		"url": "https://github.com/sponsors/neoncitylights"
	},
	"type": "module",
	"types": "./dist/index.d.ts",
	"module": "./dist/sets.mjs",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/sets.mjs"
		}
	},
	"files": [
		"dist"
	],
	"scripts": {
		"build": "vite build",
		"build:typedoc": "npx typedoc",
		"build:typedoc-watch": "npm run build:typedoc -- --watch",
		"build:all": "npm run build && npm run build:typedoc",
		"clean": "rm -rf ./dist/ ./docs/typedocs/ ./node_modules && rm -rf ./package-lock.json",
		"reinstall": "npm run clean && npm install",
		"deploy": "gh-pages -d ./docs/typedocs",
		"test": "vitest --coverage",
		"test:ci": "npm run test -- run",
		"test:ui": "npm run test -- --ui",
		"test:watch": "npm run test -- --watch",
		"test:all": "npm run test:ci && npm run lint",
		"lint": "npm run lint:md && npm run lint:es && npm run lint:pub",
		"lint:fix": "npm run lint:md-fix && npm run lint:es-fix",
		"lint:md": "markdownlint-cli2 \"*/**.md\" \"#node_modules\"",
		"lint:md-fix": "npm run lint:md -- --fix",
		"lint:md-env": "markdownlint-cli2 --version",
		"lint:es": "eslint . --ext .ts,.tsx",
		"lint:es-fix": "npm run lint:es -- --fix",
		"lint:es-env": "eslint --env-info",
		"lint:pub": "publint",
		"lint:pub-env": "publint --version",
		"prepare": "npm run build",
		"prepublishOnly": "npm run test:all",
		"preversion": "npm run lint",
		"version": "npm run lint && git add -A src",
		"postversion": "git push && git push --tags"
	},
	"devDependencies": {
		"@types/node": "^20.10.5",
		"@vitest/coverage-v8": "^1.1.0",
		"@vitest/ui": "^1.1.0",
		"eslint": "8.56.0",
		"eslint-config-neoncitylights": "^0.3.0",
		"gh-pages": "6.1.1",
		"markdownlint-cli2": "^0.11.0",
		"markdownlint-config-neoncitylights": "^0.3.0",
		"publint": "^0.2.7",
		"typedoc": "0.25.4",
		"typescript": "5.3.3",
		"vite": "^5.0.10",
		"vite-plugin-dts": "^3.6.4",
		"vitest": "^1.1.0"
	}
}
